var overlay=null;
var placeholder=YDom.get("abook-tag-placeholder");
var memberIDs=null;
var tagDialogUL;
YEvent.on(el,"click",init);
YEvent.on(el,"click",YEvent.preventDefault);
var enterKey=new YAHOO.util.KeyListener("add-tag-input",{keys:13},{fn:addTag});
enterKey.disable();

}function init(target,IDs){buildOverlay(target);
if(config.useDwr){fetchTags(IDs)
}else{nAbookfetchTags(IDs)
}overlayEvents()
}this.open=function(target,IDs){init(target,IDs)
};
function buildOverlay(target){opener=(el)?el:target;
tagDialog=document.createElement("div");
tagDialog.id="abook-tag-widget";
YDom.addClass(tagDialog,"abook-tag-widget");
YDom.insertBefore("abook-tag-widget","abook-tag-placeholder");
YDom.addClass("tag-list-container","loading");

overlay.setBody(YDom.get("abook-tagging-dialog"));
overlay.render(document.body);
tagDialog.getElementsByTagName("input")[0].value="";
tagDialog.getElementsByTagName("input")[0].focus()
}function fetchTags(IDs){memberIDs=(IDs)?IDs:config.memberIDs;
LI.later(ABookTagService,0,"getTagsForSubjects",config.context,memberIDs,{async:true,callback:function(tags){tagDialogUL=YDom.get("tag-list");
tagDialogUL.innerHTML=parseTagObject(tags);
YDom.removeClass("tag-list-container","loading")
},timeout:15000})
}function nAbookfetchTags(IDs){memberIDs=(IDs)?IDs:config.memberIDs;
if(memberIDs){if(Object.prototype.toString.apply(memberIDs)==="[object Array]"){memberIDs=memberIDs.join("|")
}else{memberIDs=memberIDs
}if(config.fetchTagsUrl){var postData="&tagContext="+escape(config.context)+"&memIds="+escape(memberIDs);
LI.asyncRequest("POST",config.fetchTagsUrl,{failure:function(){},success:function(result){if(result.responseText){var responseJson=YAHOO.lang.JSON.parse(result.responseText);
if(responseJson){tagDialogUL=YDom.get("tag-list");
tagDialogUL.innerHTML=parseNAbookTagObject(responseJson)
}YDom.removeClass("tag-list-container","loading")
}}},postData)
}}}function overlayEvents(){YEvent.on("cancel-tag-dialog","click",close);
YEvent.on(document,"click",close);
enterKey.enable();
YEvent.on("add-tag-btn","click",addTag);
var tagsList=Y$("ul",tagDialog);
YEvent.on(tagsList,"click",toggle);
YEvent.on("assign-tags","click",function(){if(YDom.get("add-tag-input").value!=""){YDom.addClass(tagDialog,"submit-without-add");
addTag()
}else{assignTag()
}})
}function close(evt,el){if(evt){var target=YEvent.getTarget(evt)
}else{closeRoutine();
return
}if(target.id=="cancel-tag-dialog"){YEvent.preventDefault(evt);
closeRoutine()
}else{if(YDom.isAncestor(tagDialog,target)||target==opener){YEvent.stopEvent(evt)
}else{closeRoutine()
}}function closeRoutine(){YEvent.removeListener(document,"click",close);
YEvent.purgeElement("abook-tagging-dialog",true);
YDom.insertBefore("abook-tagging-dialog","abook-tag-placeholder");
if(overlay){overlay.hide();
overlay.destroy()
}if(tagDialogUL){tagDialogUL.innerHTML=""
}enterKey.disable()
}}function addTag(){LI.removeAlert();
var input=YDom.get("add-tag-input");
if(input.value==""){return
}else{var isNewTag=true;
var tags=Y$("li",tagDialog);
for(var i=0;
tags.length>i;
i++){if(input.value==tags[i].innerHTML){isNewTag=false;
tags[i].className="fully-checked";
input.value="";
return
}}if(isNewTag==true){if(config.useDwr){createTag(tags,input.value)
}else{nAbookCreateTag(tags,input.value)
}}}}function createTagSuccessHandler(tags,tagName,tagId){var newTagLI=document.createElement("li");
newTagLI.innerHTML=tagName;
newTagLI.id=tagId;
YDom.addClass(newTagLI,"fully-checked");

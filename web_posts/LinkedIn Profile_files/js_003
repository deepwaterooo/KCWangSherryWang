}}function createTag(tags,tagName){var input=YDom.get("add-tag-input");
LI.later(ABookTagService,0,"createTags",config.context,tagName,{async:true,callback:function(tagId){createTagSuccessHandler(tags,tagName,tagId)
},errorHandler:function(message,ex){var error;
switch(ex.className){case"com.pronet.leo.gui.ajax.abook.TagLimitExceededAjaxException":error="tag-length-error";
break;
case"com.pronet.leo.gui.ajax.abook.TagMembersLimitExceededAjaxException":error="tag-members-length-error";
break;
case"com.pronet.leo.gui.ajax.abook.InvalidTagNameAjaxException":error="tag-length-error";
break
}input.value="";
LI.injectAlert(YDom.get(error).innerHTML,"error")
},timeout:5000})
}function nAbookCreateTag(tags,tagName){if(config.createTagUrl){var postData="&tagContext="+escape(config.context)+"&tagName="+escape(tagName);
LI.asyncRequest("POST",config.createTagUrl,{failure:function(){},success:function(result){if(result.responseText){createTagSuccessHandler(tags,tagName,result.responseText)
}}},postData)
}}function toggle(evt){var target=YEvent.getTarget(evt);
if(YDom.hasClass(target,"half-checked")||YDom.hasClass(target,"not-checked")){target.className="";
YDom.addClass(target,"fully-checked")
}else{target.className="";
YDom.addClass(target,"not-checked")
}}function assignTag(){var tagsToAssign=YDom.getElementsBy(function(elem){return(YDom.hasClass(elem,"fully-checked"))&&(YDom.getAttribute(elem,"class")!=YDom.getAttribute(elem,"data-startvalue"))
},"li",tagDialog);
var tagsIdsToAssign=[];
for(var i=0;
tagsToAssign.length>i;
i++){tagsIdsToAssign.push(tagsToAssign[i].id)
}var tagsToRemove=YDom.getElementsBy(function(elem){return(YDom.hasClass(elem,"not-checked"))&&(YDom.getAttribute(elem,"class")!=YDom.getAttribute(elem,"data-startvalue"))
},"li",tagDialog);
var tagsIdsToRemove=[];
for(var i=0;
tagsToRemove.length>i;
i++){tagsIdsToRemove.push(tagsToRemove[i].id)
}if(config.useDwr){LI.later(ABookTagService,0,"updateTagsForSubjects",config.context,tagsIdsToAssign,tagsIdsToRemove,memberIDs,{async:true,callback:function(tags){updateTags(tags)
},timeout:12000})
}else{if(config.updateTagsUrl){if(Object.prototype.toString.apply(memberIDs)==="[object Array]"){memberIDs=memberIDs.join("|")
}else{memberIDs=memberIDs
}if(tagsIdsToAssign){tagsIdsToAssign=tagsIdsToAssign.join("|")
}if(tagsIdsToRemove){tagsIdsToRemove=tagsIdsToRemove.join("|")
}var postData="&tagContext="+escape(config.context)+"&memIds="+escape(memberIDs)+"&addIds="+escape(tagsIdsToAssign)+"&remIds="+escape(tagsIdsToRemove);
LI.asyncRequest("POST",config.updateTagsUrl,{failure:function(){},success:function(result){if(result.responseText){var responseJson=YAHOO.lang.JSON.parse(result.responseText);
if(responseJson){nAbookUpdateTags(responseJson)
}}}},postData)
}}function updateTags(tags){if(config.exteriorTagList){tagDialogUL.innerHTML=parseTagObject(tags);
var exteriorUL=YDom.get(config.exteriorTagList);
var tagList="";
for(var i=0;
tags.length>i;
i++){if(tags[i].bucket=="tagsAllHave"){tagList+='<li id="'+tags[i].id+'" class="fully-checked">'+tags[i].name+",&#32;</li>&#32;"
}}exteriorUL.innerHTML="";
exteriorUL.innerHTML=tagList
}close();
if(window.abookFilters&&window.abookFilters.getContent){window.abookFilters.getContent()
}}function nAbookUpdateTags(tags){if(config.exteriorTagList){tagDialogUL.innerHTML=parseNAbookTagObject(tags);
var exteriorUL=YDom.get(config.exteriorTagList);
var tagList="";
for(var i=0;
tags.length>i;
i++){var tag=eval("("+tags[i]+")");
if(tag.bucket=="tagsAllHave"){tagList+='<li id="'+tag.id+'" class="fully-checked">'+tag.name+",&#32;</li>&#32;"
}}exteriorUL.innerHTML="";
exteriorUL.innerHTML=tagList
}close();
if(window.abookFilters&&window.abookFilters.getContent){window.abookFilters.getContent()
}}}function parseTagObject(tags){var tagList="";
var stateClass="";
for(var i=0;
tags.length>i;
i++){switch(tags[i].bucket){case"tagsAllHave":stateClass="fully-checked";
break;
case"tagsSomeHave":stateClass="half-checked";
break;
case"tagsNoneHave":stateClass="not-checked";
break
}tagList+='<li id="'+tags[i].id+'" class="'+stateClass+'" data-startvalue="'+stateClass+'">'+tags[i].name+"</li>"
}return tagList
}function parseNAbookTagObject(tags){var tagList="";
var stateClass="";
for(var i=0;
tags.length>i;
i++){var tag=eval("("+tags[i]+")");
if(tag.bucket){switch(tag.bucket){case"tagsAllHave":stateClass="fully-checked";
break;
case"tagsSomeHave":stateClass="half-checked";
break;
case"tagsNoneHave":stateClass="not-checked";
break
}}tagList+='<li id="'+tag.id+'" class="'+stateClass+'" data-startvalue="'+stateClass+'">'+tag.name+"</li>"
}return tagList
}function deleteTagAssignement(evt){var target=YEvent.getTarget(evt);
if(target.nodeName=="LI"){LI.later(ABookTagService,0,"removeSubjects",config.context,[target.id],config.memberIDs,{async:true,callback:function(){target.parentNode.removeChild(target)
},timeout:12000})
}}};

/* js/apps/Share.js */

LI.define("Share");
LI.Share=function(e,d){var d={url:(d.url)?d.url:null,title:(d.title)?d.title:LI.i18n.get("share-dialog-title"),lazyEvent:(d.lazyEvent)?d.lazyEvent:null},c=LI.SharingDialog.Scripts,a=LI.SharingDialog.Styles;
YEvent.on(e,"click",b);
if(d.lazyEvent){b(d.lazyEvent)
}function b(f){YEvent.preventDefault(f);
LI.Dialog().open({name:"sharingDialog",type:"task-modeless",width:500,content:{title:d.title,url:d.url},dependencies:{jsFiles:c,cssFiles:a}})
}};

/* js/util/LargerProfilePhoto.js */

function LargerProfilePicture(c,d){var d={vieweeId:d.vieweeId||"",imgSrc:d.imgSrc||"",imgHeight:(d.imgHeight&&d.imgHeight>=200)?d.imgHeight:200,imgWidth:(d.imgWidth&&d.imgWidth>=200)?d.imgWidth:200};
var f=true,k='<div class="loading" style="width: '+d.imgWidth+"px; height: "+d.imgHeight+'px;">&nbsp;</div>',h="<img src='"+d.imgSrc+"' height='"+d.imgHeight+"' width='"+d.imgWidth+"' />",j=50,m="TODO";
if(c.nodeName=="A"&&d.imgSrc&&d.vieweeId){var a=c.parentNode;
var b=Y$("img",c,true);
var g=document.createElement("span");
g.className="larger-profile-photo-control";
c.appendChild(g);
var p=YDom.getRegion(g);
var l=YDom.getRegion(b);
var n=parseInt(YDom.getStyle(b,"marginRight"))+parseInt(YDom.getStyle(b,"borderRightWidth"));
var o=parseInt(YDom.getStyle(b,"marginBottom"))+parseInt(YDom.getStyle(b,"borderBottomWidth"));
var i=[(l.left+l.width)-(p.width+n),(l.top+l.height)-(p.height+o)];
YDom.setXY(g,i);
function e(){WebTracking.trackLeoAction("larger-profile-picture","viewee-id:"+d.vieweeId);
if(f!=true){LI.Dialog().open({id:"larger-profile-photo",type:"no-header",width:d.imgWidth+j,className:"larger-profile-photo",content:{html:h,title:""}})
}else{LI.Dialog().open({id:"larger-profile-photo",type:"no-header",width:d.imgWidth+j,className:"larger-profile-photo",content:{html:k,title:""}});
var q=new Image();
YEvent.on(q,"load",function(){LI.Dialog().swap({id:"larger-profile-photo",type:"no-header",width:d.imgWidth+j,className:"larger-profile-photo",content:{html:h,title:""}});
f=false
});
q.src=d.imgSrc
}}YAHOO.util.Event.addListener(a,"mouseover",function(q){YAHOO.util.Dom.addClass(a,"profile-photo-hover")
});
YAHOO.util.Event.addListener(a,"mouseout",function(q){YAHOO.util.Dom.removeClass(a,"profile-photo-hover")
});
YAHOO.util.Event.addListener(c,"click",function(q){YEvent.preventDefault(q);
e()
})
}};
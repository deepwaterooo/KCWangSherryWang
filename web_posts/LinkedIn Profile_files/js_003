LI.define("AbookTaggingDialog");
LI.AbookTaggingDialog=function(el,config){var config={memberIDs:config.memberIDs||null,exteriorTagList:(config.exteriorTagList)?config.exteriorTagList:null,allowExteriorDelete:(config.allowExteriorDelete)?config.allowExteriorDelete:null,context:config.context,useDwr:(config.useDwr!=="undefined"&&config.useDwr===false)?false:true,createTagUrl:config.createTagUrl||null,fetchTagsUrl:config.fetchTagsUrl||null,updateTagsUrl:config.updateTagsUrl||null};
